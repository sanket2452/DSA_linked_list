/*
Following is the class structure of the Node class:

class Node
{
public:
    int data;
    Node *next;
    Node()
    {
        this->data = 0;
        next = NULL;
    }
    Node(int data)
    {
        this->data = data; 
        this->next = NULL;
    }
    Node(int data, Node* next)
    {
        this->data = data;
        this->next = next;
    }
};
*/
/*using fast and slow pointer TC O(n) SC O(1)*/
Node *findMiddle(Node *head) {
    // Write your code here
    Node *slow=head;
    Node *fast=head;
    while(fast->next!=NULL){
        fast=fast->next;
        if(fast->next!=NULL) 
           fast=fast->next;
        slow=slow->next;
    }
    
    
    return slow;
}

